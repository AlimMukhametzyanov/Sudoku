<Window x:Class="Sudoku.SavedGames"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SavedGames" Height="200" Width="334" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" KeyDown="Window_KeyDown">
    <Grid>
        <ComboBox Name="cmbSetOfGames" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="298"/>
        <Label Content="Id | Название | Дата изменения | Сложность | Время" HorizontalAlignment="Left" Margin="6,12,0,0" VerticalAlignment="Top" Width="302" HorizontalContentAlignment="Center"/>
        <Button Name="btnOk" Content="Ок" HorizontalAlignment="Left" Margin="208,123,0,0" VerticalAlignment="Top" Width="88" Height="27" Click="btnOk_Click"/>
        <Button Name="btnBack" Content="Назад" HorizontalAlignment="Left" Margin="22,123,0,0" VerticalAlignment="Top" Width="88" Height="27" Click="btnBack_Click"/>
        <Button Name="btnDelete" Content="Удалить" HorizontalAlignment="Left" Margin="115,123,0,0" VerticalAlignment="Top" Width="88" Height="27" Click="btnDelete_Click" Background="#FFFF7373"/>
    </Grid>
</Window>
